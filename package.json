    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.13",
    "katex": "^0.12.0",
    "node-sass": "^4.13.1",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
