    "gatsby-remark-images": "^3.1.49",
    "gatsby-remark-images-medium-zoom": "^1.4.0",
    "gatsby-remark-katex": "^3.3.10",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-remark-reading-time": "^1.1.0",
